<!DOCTYPE html>
<html>
<head>
{% block head %}
<title>{{ APP_NAME }}</title>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
<link rel="stylesheet" href="{{ BOOTSTRAP_CSS }}" />
<link rel="stylesheet" href="{{ JQUERY_FILEUPLOAD_CSS }}" />
<link rel="stylesheet" href="{{ FONT_AWESOME_CSS }}" />
<link rel="stylesheet" href="{{ VIDEOJS_CSS }}" />
<link href='https://fonts.googleapis.com/css?family=Raleway:400,700,500' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="{{ ASSETS_CSS_BOOTSTRAP_CADENZA }}" />
<link rel="stylesheet" href="{{ ASSETS_CSS_CADENZA }}" />
<link rel="stylesheet" href="{{ ASSETS_CSS_FAST }}" />
{% if include_css_components %}
<link rel="stylesheet" href="{{ ASSETS_CSS_CADENZA_COMPONENTS }}" />
{% endif %}
{% if include_css_components_admin %}
<link rel="stylesheet" href="{{ ASSETS_CSS_CADENZA_COMPONENTS_ADMIN }}" />
{% endif %}
<script type="text/javascript" src="{{ JQUERY_JS }}"></script>
<script type="text/javascript" src="{{ JQUERY_UI_WIDGET_JS }}"></script>
<script type="text/javascript" src="{{ JQUERY_IFRAME_TRANSPORT_JS }}"></script>
<script type="text/javascript" src="{{ JQUERY_FILEUPLOAD_JS }}"></script>
<script type="text/javascript" src="{{ JQUERY_FILEUPLOAD_PROCESS_JS }}"></script>
<script type="text/javascript" src="{{ JQUERY_FILEUPLOAD_VALIDATION_JS }}"></script>
<script type="text/javascript" src="{{ AUTOSIZE_JS }}"></script>
<script type="text/javascript" src="{{ VIDEOJS_JS }}"></script>
<script type="text/javascript" src="{{ TYPEHEADJS_JS }}"></script>
<script type="text/javascript" src="{{ BOOTSTRAP_JS }}"></script>
<script type="text/javascript" src="{{ MODIFIED_CORS_UPLOAD_SAMPLE_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_PAGE_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_FILEUPLOADS_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_DRIVEUPLOADS_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_DRIVE_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_AUTOCOMPLETE_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_ACTIONBUTTONS_JS }}"></script>
<script type="text/javascript" src="{{ CADENZA_WIDGETS_JS_DIR }}/filter.js"></script>
<script type="text/javascript" src="{{ CADENZA_WIDGETS_JS_DIR }}/pagination.js"></script>
<script type="text/javascript" src="{{ CADENZA_WIDGETS_JS_DIR }}/sortable_dropdown_select.js"></script>
<script type="text/javascript" src="{{ CADENZA_WIDGETS_JS_DIR }}/sortable_label.js"></script>
{% for component in js_components %}
<script type="text/javascript" src="{{ CADENZA_COMPONENTS_JS_DIR }}/{{ component }}.js"></script>
{% endfor %}
{% for admin_component in js_admin_components %}
<script type="text/javascript" src="{{ CADENZA_ADMIN_COMPONENTS_JS_DIR }}/{{ admin_component }}.js"></script>
{% endfor %}
<script type="text/javascript">
$(document).ready(function() {
	_.init('{{ CADENZA_URL }}', '{{ GOOGLE_CLIENT_ID }}', '{{ GOOGLE_BROWSER_API_KEY }}', {{ FILEUPLOAD_MAX_CHUNK_SIZE }}, {{ TEXT.js|json_encode|raw }}, {{ ICONS.btn|json_encode|raw }}, '{{ VIDEOJS_SWF }}', {{ js_components|json_encode|raw }}, {{ js_admin_components|json_encode|raw }});
	{% if js_alert_message is not empty %}_.alert('{{ js_alert_message }}');{% endif %}
	{% if is_redirect and destination is not empty %}window.location = '{{ destination }}';{% endif %}
});
</script>
{% if GOOGLE_ANALYTICS_ENABLED %}
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-10430250-6', 'auto');
  ga('send', 'pageview');
</script>
{% endif %}
{% endblock %}
</head>
<body class="{{ body_class }}">
{% block body %}{% endblock %}
</body>
</html>
