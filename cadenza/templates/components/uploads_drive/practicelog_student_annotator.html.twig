{% extends "components/upload_drive.html.twig" %}

{% set upload_id = 'id_upload_practicelog_student_annotator_' ~ practice.practice_id %}
{% set upload_class = 'upload-practicelog-student-annotator' %}
{% set practice_id = practice.practice_id %}
{% set uploaded_drive_files = { 'practice_id': practice.practice_id, 'annotator_file_id': practice.annotator_file_id, 'annotator_title': practice.annotator_title } %}
{% set is_readonly = user.user_type != 'student' or not is_link_connected %}
{% set can_annotate = is_link_connected %}
{% set is_practicing = false %}
{% set user_email = user.email %}
{% set open_on_load = show_annotation_modal %}

{% block heading %}
	<h5 class="color-env">{{ TEXT.label['media_annotator']|upper }}</h5>
{% endblock %}